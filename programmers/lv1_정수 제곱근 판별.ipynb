   "source": [
    "def solution(n):\n",
    "    answer = 0\n",
    "    \n",
    "    i = 1\n",
    "    while i**2 < n:\n",
    "        if i**2 == n:\n",
    "            answer = i+1\n",
    "            break\n",
    "        i+=1\n",
    "    \n",
    "    if answer == 0:\n",
    "        return -1\n",
    "    else:\n",
    "        return answer**2\n",
    "\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "print(solution(n))"
   ]
