   "source": [
    "def solution(absolutes, signs):\n",
    "    answer = 0\n",
    "    length = len(absolutes)\n",
    "    for i in range(length):\n",
    "        if signs[i]:\n",
    "            answer += absolutes[i]\n",
    "        else:\n",
    "            answer -= absolutes[i]\n",
    "        \n",
    "    return answer"
   ]
