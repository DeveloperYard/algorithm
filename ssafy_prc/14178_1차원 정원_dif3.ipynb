   "source": [
    "def sol(n, d):\n",
    "    ans = 0\n",
    "    ran = 2*d+1\n",
    "    \n",
    "    i = 1\n",
    "    \n",
    "    while i*ran < n:\n",
    "        i+=1\n",
    "    \n",
    "    ans = i\n",
    "    return ans\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "for i in range(1, n+1):\n",
    "    n, d = map(int, input().split())\n",
    "    res = sol(n, d)\n",
    "    print(\"#\"+str(i)+ \" \"+str(res))"
   ]
